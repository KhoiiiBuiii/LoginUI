<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.DependantPageControl">
    <children>
        <Label fx:id="welcomeLabel" styleClass="welcome-label" text="Welcome" />
        <Button fx:id="logoutButton" styleClass="logout-button" text="Logout" />
        <VBox alignment="CENTER" spacing="10">
            <Label styleClass="services-label" text="Our Services" />
            <Button fx:id="selfInfoButton" styleClass="service-button" text="Self Information" />
            <Button fx:id="viewClaimsButton" styleClass="service-button" text="View Claims" />
        </VBox>
    </children>
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
</VBox>
