<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PolicyHolderPageControl">
    <children>
        <Label fx:id="welcomeLabel" styleClass="welcome-label" text="Welcome" />
        <Button fx:id="logoutButton" styleClass="logout-button" text="Logout" onAction="#handleLogoutButtonAction"/>
        <VBox alignment="CENTER" spacing="10">
            <Label styleClass="services-label" text="Our Services" />
            <Button fx:id="selfInfoButton" styleClass="service-button" text="Self Information" onAction="#handleSelfInfoButton" />
            <Button fx:id="manageClaimsButton" styleClass="service-button" text="Manage Claims" onAction="#handleManageClaimsButton" />
            <Button fx:id="manageDependantsButton" styleClass="service-button" text="Manage Dependants" onAction="#handleManageDependantsButton" />
        </VBox>
        <VBox fx:id="additionalContentContainer" alignment="CENTER" spacing="10"/>
    </children>
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
</VBox>
